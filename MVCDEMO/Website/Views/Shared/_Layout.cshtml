@using Sitecore.Mvc
@using Sitecore.Mvc.Analytics.Extensions
@using Sitecore.Mvc.Presentation
@using System.Threading.Tasks
@model RenderingModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html class="@(Sitecore.Context.PageMode.IsPageEditor ? "inexpeditor" : "notexpeditor")">
<head>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/Images/favicon.ico" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>
        @Html.Sitecore().Field("Title", new { DisableWebEdit = true })
    </title>
    @Html.Sitecore().VisitorIdentification()
    <link href="@Url.Content("~/Website/Content/Css/bootstrap.min.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Website/Content/Css/style.css")" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="@Url.Content("~/Website/Content/Js/jquery.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Website/Content/Js/bootstrap.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Website/Content/Js/Smoothstate.js")"></script>

</head>

<body>
    <div id="main" class="row col-sm-12 content">
        @RenderBody()
        <footer>
            <script src="@Url.Content("~/Website/Content/Js/mvc3.js")" type="text/javascript"></script>
        </footer>
        <div class="o-loading-spinner"></div>
        <div class="overlay"></div>
    </div>
</body>
</html>
